@model Doe.Ls.ProjectTemplate.Core.BL.Models.Extensions.UserInfoExtension
@{
    ViewBagWrapper.InfoBag.SetTitle("My profile",ViewData);
    var user = ViewBagWrapper.UserInfoExtensionBag.GetCurrentUser(ViewData);
    var task = ViewBagWrapper.TaskBag.GetCurrentTask(ViewData);
}

<div class="page-header">
    <h2>@ViewBagWrapper.InfoBag.GetTitle(ViewData)</h2>
</div>

<div id="formTab" class="card.card-body">
    <form class="" data-tracking-enabled="true" role="form" method="POST" id="form-sysUser-details">
        @Html.AntiForgeryToken()
        
        @Html.Partial("_partial/_myProfile")
        
    </form>
</div>


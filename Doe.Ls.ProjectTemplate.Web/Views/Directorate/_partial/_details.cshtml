

@model Directorate
@{
    ViewBagWrapper.InfoBag.SetTitle("Directorate Details", ViewData);
    var user = ViewBagWrapper.UserInfoExtensionBag.GetCurrentUser(ViewData);
    var task = ViewBagWrapper.TaskBag.GetCurrentTask(ViewData);

    
}



@Html.HiddenFor(model => model.DirectorateId)

<div class="form-group">
        <span class="col-lg-4 col-form-label">@Html.DisplayNameFor(model => model.DirectorateId)</span>
       
        <div class="col-lg-8"><p class="form-control-plaintext">@Html.DisplayFor(model => model.DirectorateId)</p></div>
        </div>
<div class="form-group">
    	    <span class="col-lg-4 col-form-label">@Html.DisplayNameFor(model => model.Executive)</span>
   
        <div class="col-lg-8"><p class="form-control-plaintext">@Html.DisplayFor(model => model.Executive.ExecutiveTitle)</p>
    </div>
        </div>
<div class="form-group">
        <span class="col-lg-4 col-form-label">@Html.DisplayNameFor(model => model.DirectorateName)</span>
       
        <div class="col-lg-8"><p class="form-control-plaintext">@Html.DisplayFor(model => model.DirectorateName)</p></div>
        </div>





 

@model BusinessUnit
@{
    ViewBagWrapper.InfoBag.SetTitle("Detail Business unit",ViewData);
    var user = ViewBagWrapper.UserInfoExtensionBag.GetCurrentUser(ViewData);
    var task = ViewBagWrapper.TaskBag.GetCurrentTask(ViewData);
}

<div class="page-header">
    <h2>@ViewBagWrapper.InfoBag.GetTitle(ViewData)</h2>
</div>

<div id="formTab" class="card.card-body">
    <form class="" role="form" method="POST" id="form-businessUnit-details">
        @Html.AntiForgeryToken()
        <p>
            <a href="@Url.Action("Edit", new { id  = Model.BUnitId })" class="btn btn-primary">Edit</a>
            <a href="@Url.Action("Index")" class="btn btn-primary">BusinessUnit List <span class="glyphicon glyphicon-th-list"></span></a>
        </p>
    
        @Html.Partial("_partial/_details")
        <p>
            <a href="@Url.Action("Edit", new { id  = Model.BUnitId })" class="btn btn-primary">Edit</a>
            <a href="@Url.Action("Index")" class="btn btn-primary">BusinessUnit List <span class="glyphicon glyphicon-th-list"></span></a>
        </p>
    </form>
</div>


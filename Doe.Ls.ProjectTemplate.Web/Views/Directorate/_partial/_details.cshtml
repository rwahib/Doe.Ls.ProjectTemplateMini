        <div class="col-lg-8"><p class="form-control-plaintext">@Html.DisplayFor(model => model.DirectorateOverview)</p></div>
        </div>

<div class="form-group">
    <label class="col-lg-2 col-form-label">Locations
    </label>

    <div class="col-lg-10">
        <p class="form-control-plaintext">@foreach (var loc in Model.Locations)
                                       {
                                           if (loc != null)
                                           {
                                               @loc.Name
                                               <br>
                                           }
                                       }
        </p>
    </div>
</div>



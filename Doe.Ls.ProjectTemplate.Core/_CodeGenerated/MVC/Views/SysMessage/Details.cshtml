



 

@model SysMessage
@{
    ViewBagWrapper.InfoBag.SetTitle("Detail Sys message",ViewData);
    var user = ViewBagWrapper.UserInfoExtensionBag.GetCurrentUser(ViewData);
    var task = ViewBagWrapper.TaskBag.GetCurrentTask(ViewData);
}

<div class="page-header">
    <h2>@ViewBagWrapper.InfoBag.GetTitle(ViewData)</h2>
</div>

<div id="formTab" class="card.card-body">
    <form class="" role="form" method="POST" id="form-sysMessage-details">
        @Html.AntiForgeryToken()
        <p>
            <a href="@Url.Action("Edit", new { code  = Model.Code })" class="btn btn-primary">Edit</a>
            <a href="@Url.Action("Index")" class="btn btn-primary">SysMessage List <span class="glyphicon glyphicon-th-list"></span></a>
        </p>
    
        @Html.Partial("_partial/_details")
        <p>
            <a href="@Url.Action("Edit", new { code  = Model.Code })" class="btn btn-primary">Edit</a>
            <a href="@Url.Action("Index")" class="btn btn-primary">SysMessage List <span class="glyphicon glyphicon-th-list"></span></a>
        </p>
    </form>
</div>


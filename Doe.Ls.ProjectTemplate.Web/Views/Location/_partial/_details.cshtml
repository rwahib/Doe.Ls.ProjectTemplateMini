

@model Location
@{
    ViewBagWrapper.InfoBag.SetTitle("Location Details", ViewData);
    var user = ViewBagWrapper.UserInfoExtensionBag.GetCurrentUser(ViewData);
    var task = ViewBagWrapper.TaskBag.GetCurrentTask(ViewData);
}



@Html.HiddenFor(model => model.LocationId)

<div class="form-group">
    <span class="col-lg-4 col-form-label">@Html.DisplayNameFor(model => model.LocationId)</span>

    <div class="col-lg-8"><p class="form-control-plaintext">@Html.DisplayFor(model => model.LocationId)</p></div>
</div>
